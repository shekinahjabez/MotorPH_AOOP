	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MotorPH_AOOP"/>
	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<parameter name="employee_id" class="java.lang.Integer"/>
	<parameter name="month" class="java.lang.Integer"/>
	<parameter name="year" class="java.lang.Integer"/>
	<parameter name="LogoPath" class="java.net.URL"/>
	<query language="SQL"><![CDATA[SELECT 
    eh.employee_id,
    CONCAT(e.firstname, ' ', e.lastname) AS full_name,
    ep.position,
    eh.date,
    TO_CHAR(eh.date, 'Dy') AS day,
    TO_CHAR(eh.time_in, 'HH24:MI') AS time_in,
    TO_CHAR(eh.time_out, 'HH24:MI') AS time_out,
    TO_CHAR((eh.time_out - eh.time_in), 'HH24:MI') AS total_hours_worked,
    CASE 
        WHEN eh.time_in IS NULL OR eh.time_out IS NULL THEN
            CASE 
                WHEN TO_CHAR(eh.date, 'Dy') IN ('Sat', 'Sun') THEN 'Weekend' 
                ELSE 'Absent' 
            END
        ELSE 'Present'
    END AS remarks
FROM 
    employee_hours eh
JOIN 
    employee e ON e.employee_id = eh.employee_id
JOIN 
    employee_position ep ON ep.id = e.position
WHERE
    eh.employee_id = $P{employee_id}
    AND EXTRACT(MONTH FROM eh.date) = $P{month}
    AND EXTRACT(YEAR FROM eh.date) = $P{year}
ORDER BY eh.date;]]></query>
